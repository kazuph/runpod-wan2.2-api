{
  "$schema": "http://json-schema.org/draft-04/hyper-schema#",
  "type": "object",
  "properties": {
    "readme": {
      "type": "string",
      "title": "Wan 2.2 FLF (First-Last Frame): Generate videos from start and end frames",
      "author": "Ang Wang, Baole Ai, Bin Wen, Chaojie Mao, Chen-Wei Xie, Di Chen, Feiwu Yu, Haiming Zhao, Jianxiao Yang, Jianyuan Zeng",
      "code": "https://github.com/Wan-Video/Wan2.2",
      "paper": "https://arxiv.org/abs/2503.20314",
      "page": "https://wan.video/",
      "tags": [
        "First-Last Frame Video Generation"
      ],
      "widget": "readme"
    },
    "upload_start": {
      "type": "string",
      "widget": "upload",
      "upload_url": "https://tost.ai/api/jobs/upload",
      "property": "start_image"
    },
    "start_image": {
      "type": "string",
      "description": "Start Frame Image URL",
      "widget": "textarea"
    },
    "upload_end": {
      "type": "string",
      "widget": "upload", 
      "upload_url": "https://tost.ai/api/jobs/upload",
      "property": "end_image"
    },
    "end_image": {
      "type": "string",
      "description": "End Frame Image URL",
      "widget": "textarea"
    },
    "positive_prompt": {
      "type": "string",
      "description": "Positive Prompt",
      "widget": "textarea",
      "default": "smooth transition animation between frames"
    },
    "negative_prompt": {
      "type": "string",
      "description": "Negative Prompt",
      "widget": "hidden",
      "default": "static, blurry, low quality, mouth opening, talking"
    },
    "width": {
      "type": "integer",
      "widget": "select",
      "description": "Width",
      "oneOf": [
        {
          "description": "576",
          "enum": [
            576
          ]
        },
        {
          "description": "720",
          "enum": [
            720
          ]
        },
        {
          "description": "1024",
          "enum": [
            1024
          ]
        }
      ],
      "default": 720
    },
    "height": {
      "type": "integer",
      "widget": "select",
      "description": "Height",
      "oneOf": [
        {
          "description": "576",
          "enum": [
            576
          ]
        },
        {
          "description": "720",
          "enum": [
            720
          ]
        },
        {
          "description": "1280",
          "enum": [
            1280
          ]
        }
      ],
      "default": 1280
    },
    "length": {
      "type": "integer",
      "description": "Length (frames)",
      "widget": "select",
      "oneOf": [
        {
          "description": "49 frames (~2 seconds)",
          "enum": [
            49
          ]
        },
        {
          "description": "65 frames (~2.7 seconds)",
          "enum": [
            65
          ]
        },
        {
          "description": "81 frames (~3.4 seconds)",
          "enum": [
            81
          ]
        },
        {
          "description": "97 frames (~4 seconds)",
          "enum": [
            97
          ]
        }
      ],
      "default": 81
    },
    "batch_size": {
      "type": "integer",
      "description": "Batch Size",
      "widget": "hidden",
      "default": 1
    },
    "shift": {
      "type": "number",
      "description": "Shift",
      "default": 8.0
    },
    "cfg": {
      "type": "number",
      "description": "CFG",
      "default": 4.0
    },
    "steps": {
      "type": "integer",
      "description": "Steps",
      "widget": "hidden",
      "default": 20
    },
    "seed": {
      "type": "integer",
      "description": "Seed (if 0 random)",
      "default": 42
    },
    "fps": {
      "type": "integer",
      "description": "FPS",
      "default": 24
    },
    "example": {
      "type": "string",
      "image1": "https://files.catbox.moe/flf1.mp4",
      "image2": "https://files.catbox.moe/flf2.mp4",
      "image3": "https://files.catbox.moe/flf3.mp4",
      "image4": "https://files.catbox.moe/flf4.mp4",
      "video_aspect_ratio": "60%",
      "line1": "Video: 720x1280 pixels, 3.4 seconds | Gen Time: ~3 minutes",
      "widget": "example"
    }
  },
  "buttons": [
    {
      "id": "enter",
      "label": "ðŸŽ¬ Generate FLF Video"
    }
  ]
}